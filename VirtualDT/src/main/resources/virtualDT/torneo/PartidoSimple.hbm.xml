<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<!--         private Equipo equipoLocal, equipoVisitante; -->
        
<hibernate-mapping>
	<class name ="virtualDT.torneo.PartidoSimple" table="PartidosSimples">
		<id name ="idP" type="java.lang.Integer">
			<column name="ID_PARTIDO"></column>
			<generator class="identity"></generator>
		</id>
		<many-to-one name="equipoLocal" column="Equipo_Local"/>
		<many-to-one name="equipoVisitante" column="Equipo_Visitante"/>
		
	</class>
</hibernate-mapping>